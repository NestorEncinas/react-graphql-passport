{
  "compilerOptions": {
    "lib": ["dom", "es2016", "es2017", "esnext.asynciterable"],
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "jsx": "preserve"
  },
  "include": ["./server", "./src"],
  "exclude": ["node_modules"]
}
